{
  "manifest_version": 3,
  "name": "Developer Notes",
  "version": "1.0",
  "description": "Take notes in Chrome DevTools Panel",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "art/128.png",
    "48": "art/48.png",
    "16": "art/16.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html"
}
